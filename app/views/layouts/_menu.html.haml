%nav.menu
  %ul.buttons
  - if user_signed_in?
    - if is_company?(current_user.id)
      %li= link_to "Jobs", job_index_path, class: "button"
    %li= link_to "Profile", profile_index_path, class: "button"
    %li= link_to "Available Skills", skill_index_path, class: "button"
    %li= button_to "Log Out", destroy_user_session_path, class: "log_out_Button", method: :delete 
  - else
    %li= button_to "Log In", new_user_session_path, class: "log_out_Button"
    