%div.container
  %div.job-feed
    %h1
      Jobs Feed

    %ul
      %h3
      - @jobs.each do |job|
        %h3
          %li= link_to job.title, view_job_path(job), remote: true, class: "button"
          - if user_signed_in? && is_user?(current_user.id)
            = "Matching by: #{matching_percentage(job)}%"


  %div.job-info


= render "profile/user_applications"